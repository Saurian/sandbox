{default $projects = []}
{block content}
	<div id="banner">
		<h1 n:block=title>E - Rovnice</h1>
	</div>

	<div id="content">
+		<h2>Výpis projektů <small class="pull-right"><a n:href="Admin:" class="btn btn-xs btn-primary">Administrace</a></small></h2>

		<div class="boxes">
			<div class="box well" n:foreach="$projects as $project">
				<h2><a n:href="detail $project->id"><img src="data:image/png;base64," alt="">{$project->name}</a></h2>
				<p>Datum odevzdání projektu {$project->release_date} .<p>
					<small n:if="$project->web" class="bg-success">jedná se o webový projekt</small>
					<small n:if="!$project->web" class="bg-danger">nejedná se o webový projekt</small>
			</div>
		</div>

		<div class="boxes" n:if="count($projects) == 0">
			<div>
				<div class="alert alert-danger" role="alert">Nejsou žádné projekty</div>
			</div>
		</div>

	</div>
{/block}
